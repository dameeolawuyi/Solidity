 //pragma solidity >=0.4.22 <0.9.0;

 pragma solidity 0.8.10;

contract WeiWallet{
    address public owner;
    constructor() {
        owner= msg.sender;
    }
    
    //to recieve ether
    
    function recieve() external payable{
        emit WithoutData(msg.value);
    }
    //to send ether
        
    function sendEther(address) external payable(uint) {
        require(amount<= address(this). balance "sorry, insufficient balance"),
        recieve.transfer(_amount);
    }
    
   //to store balances

    Mapping{

    mapping(address=>uint) external userMap;
    function getBalance() external view returns (uint)
    balances{msg.userMap} =120;
}
    
}
